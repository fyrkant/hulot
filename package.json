{
  "name": "compotest",
  "version": "0.0.4",
  "description": "an easy way to generate react components and tests",
  "main": "index.js",
  "bin": {
    "compotest": "bin/index.js"
  },
  "scripts": {
    "test": "istanbul cover _mocha './tests/**/*-spec.js' --report lcovonly -- -R spec './bin/**/*.js' && cat ./coverage/lcov.info | coveralls",
    "test:dev": "mocha './tests/**/*-spec.js'",
    "test:watch": "mocha './tests/**/*-spec.js' --reporter min -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mw222rs/compotest.git"
  },
  "keywords": [
    "react",
    "mocha",
    "test",
    "generator",
    "component"
  ],
  "author": "Mattias Wikstrom <fyrkant@gmail.com> (http://mattias.rocks)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mw222rs/compotest/issues"
  },
  "homepage": "https://github.com/mw222rs/compotest#readme",
  "devDependencies": {
    "coveralls": "^2.11.6",
    "expect": "^1.13.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.2",
    "mock-fs": "^3.7.0"
  }
}
